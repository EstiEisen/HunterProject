section {
  padding: 20px 0;
  border-bottom: 1px solid #c9c7c7;
}

.box {
  display: flex;
  padding: 20px 0;
  flex-direction: column;
  align-items: center;
  border-radius: 8px;
  box-shadow: 0 4px 43px 0 rgba(0, 0, 0, 0.04);
  text-align: center;
}

.box-container:nth-child(1) > .box {
  background-color: rgba(87, 189, 190, 1)
}

.box-container:nth-child(2) > .box {
  background-color: rgba(251, 103, 131, 1);
}

.box-container:nth-child(3) > .box {
  background-color: #41a8ec;
}

.box-container:nth-child(4) > .box {
  background-color: rgba(249, 197, 135, 1);
}

.box > img {
  height: 50px;
}

.box > p {
  margin: 0;
  font-size: 18px;
  color: #fff;
}

.box > p:last-child {
  font-weight: bold;
}

#feedback > p {
  color: #a4a4a3;
}

#doughnutGraphContainer > span {
  position: absolute;
  top: 100px;
  left: 50%;
  transform: translate(-50%, -50%);
  color: #747474;
  font-size: 24px;
}

#doughnutGraphContainer p {
  margin: 0;
}
.example-month-picker .mat-calendar-period-button {
