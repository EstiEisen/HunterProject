 <button class="dialog-close mb-3" mat-dialog-close>
    <i class="fa fa-times"></i>
  </button>
  <p class="h3">פניה לקופה</p>
  <hr>



 <div class="d-flex">
 <div class="w-75 ml-3">
<p class="mb-0">
  חברה מנהלת:
  {{this.feedback.manufacturer.name}}
</p>
   <p class="mb-0">
     סכום קובץ:
     {{this.feedback.sumFile}}
   </p>
   <p class="mb-0">
     שם קובץ:
     {{this.feedback.fileName}}
   </p>
   <p class="mb-0">
     סוג שגיאה:

   </p>
 </div>
 <div class="w-25 d-flex justify-content-center align-items-center flex-column">


   <mat-form-field>
     <mat-select placeholder="בחר איש קשר">
       <mat-option *ngFor="let contact of contacts" [value]="contact.id">
         {{ contact.text }}
       </mat-option>
     </mat-select>
   </mat-form-field>

   <mat-form-field>
     <mat-select placeholder="בחר קובץ מערכת">
       <mat-option *ngFor="let file of files" [value]="file.id">
         {{ file.text }}
       </mat-option>
     </mat-select>
   </mat-form-field>


 </div>
 </div>

 <div class="d-flex">
   <mat-form-field class="w-75 ml-3">
     <textarea matInput placeholder="הערות"></textarea>
   </mat-form-field>
   <div class="w-25 d-flex justify-content-end align-items-center flex-column">
     <button class="btn btn-primary mb-1">שלח פניה</button>
   </div>

 </div>
